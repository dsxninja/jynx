.stickers(v-if="panel.stickers")
    ul.list
        li(v-for="(sticker, idx) of stickers", :key="idx", @click="sendMessage('sticker', sticker.url)")
            img(:src="sticker.url")
    .add
        input(type="text", placeholder="Ссылка на изображение", v-model="sticker.url", @keyup.enter="addSticker()")
        .btn(data-ui-title="Добавить стикер", @click="addSticker()")
            i.uil.uil-image-plus